{
  "name": "store-poc-node",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link": "^1.0.0",
    "apollo-link-http": "^1.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-server": "^0.12.0",
    "node-fetch": "^1.7.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
